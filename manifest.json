{
    "name": "Casey and Amy's Extension",
    "version": "1.0",
    "description": "Our Google Chrome Extension",
    "manifest_version": 2,
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "scary-obj"
    }
  }